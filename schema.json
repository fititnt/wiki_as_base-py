{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Wiki-as-base JSON-LD API output",
  "description": "Early draft. See https://github.com/fititnt/wiki_as_base-py",
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^@": {
      "type": [
        "string",
        "object"
      ]
    }
  },
  "properties": {
    "$id": {
      "type": "string"
    },
    "$schema": {
      "type": "string"
    },
    "data": {
      "type": [
        "array",
        "object"
      ]
    },
    "meta": {
      "type": "object"
    },
    "error": {
      "type": "object"
    }
  }
}